<resources xmlns:tools="http://schemas.android.com/tools">
    <string-array name="pref_entries_autosync">
        <item>@string/autosync_on</item>
        <item>@string/autosync_only_on_wifi</item>
        <item>@string/autosync_off</item>
    </string-array>

    <string-array name="pref_entryvalues_autosync" translatable="false">
        <item>ON</item>
        <item>WIFI</item>
        <item>OFF</item>
    </string-array>

    <string-array name="pref_entries_resurvey_intervals">
        <item>@string/resurvey_intervals_less_often</item>
        <item>@string/resurvey_intervals_default</item>
        <item>@string/resurvey_intervals_more_often</item>
    </string-array>

    <string-array name="pref_entryvalues_resurvey_intervals" translatable="false">
        <item>LESS_OFTEN</item>
        <item>DEFAULT</item>
        <item>MORE_OFTEN</item>
    </string-array>

    <string-array name="pref_entries_theme_select" translatable="false" tools:ignore="InconsistentArrays">
        <item>@string/theme_automatic</item>
        <item>@string/theme_light</item>
        <item>@string/theme_dark</item>
    </string-array>

    <string-array name="pref_entryvalues_theme_select" translatable="false" tools:ignore="InconsistentArrays">
        <item>AUTO</item>
        <item>LIGHT</item>
        <item>DARK</item>
    </string-array>

</resources>
